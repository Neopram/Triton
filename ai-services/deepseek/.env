# Model Configuration
DEEPSEEK_MODEL=deepseek-ai/deepseek-coder-33b-instruct
DEEPSEEK_MAX_TOKENS=2048
DEEPSEEK_DEFAULT_TEMPERATURE=0.7
DEEPSEEK_TOP_P=0.9
